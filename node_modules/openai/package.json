{
  "name": "openai",
  "version": "5.0.0-beta.0",
  "description": "The official TypeScript library for the OpenAI API",
  "author": "OpenAI <support@openai.com>",
  "types": "./index.d.ts",
  "main": "./index.js",
  "type": "commonjs",
  "repository": "github:openai/openai-node",
  "license": "Apache-2.0",
  "packageManager": "yarn@1.22.22",
  "files": [
    "**/*"
  ],
  "private": false,
  "scripts": {
    "test": "./scripts/test",
    "build": "./scripts/build",
    "format": "prettier --write --cache --cache-strategy metadata . !dist",
    "tsn": "ts-node -r tsconfig-paths/register",
    "lint": "./scripts/lint",
    "fix": "./scripts/format"
  },
  "dependencies": {},
  "resolutions": {
    "synckit": "0.8.8"
  },
  "imports": {
    "openai": ".",
    "openai/*": "./src/*"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "types": "./index.d.mts",
      "default": "./index.mjs"
    },
    "./_vendor/*.mjs": {
      "default": [
        "./_vendor/*.mjs",
        "./_vendor/*/index.mjs"
      ]
    },
    "./_vendor/*.js": {
      "default": [
        "./_vendor/*.js",
        "./_vendor/*/index.js"
      ]
    },
    "./_vendor/*": {
      "import": [
        "./_vendor/*.mjs",
        "./_vendor/*/index.mjs"
      ],
      "require": [
        "./_vendor/*.js",
        "./_vendor/*/index.js"
      ]
    },
    "./api-promise": {
      "import": "./api-promise.mjs",
      "require": "./api-promise.js"
    },
    "./api-promise.js": {
      "default": "./api-promise.js"
    },
    "./api-promise.mjs": {
      "default": "./api-promise.mjs"
    },
    "./azure": {
      "import": "./azure.mjs",
      "require": "./azure.js"
    },
    "./azure.js": {
      "default": "./azure.js"
    },
    "./azure.mjs": {
      "default": "./azure.mjs"
    },
    "./beta/*.mjs": {
      "default": [
        "./beta/*.mjs",
        "./beta/*/index.mjs"
      ]
    },
    "./beta/*.js": {
      "default": [
        "./beta/*.js",
        "./beta/*/index.js"
      ]
    },
    "./beta/*": {
      "import": [
        "./beta/*.mjs",
        "./beta/*/index.mjs"
      ],
      "require": [
        "./beta/*.js",
        "./beta/*/index.js"
      ]
    },
    "./client": {
      "import": "./client.mjs",
      "require": "./client.js"
    },
    "./client.js": {
      "default": "./client.js"
    },
    "./client.mjs": {
      "default": "./client.mjs"
    },
    "./error": {
      "import": "./error.mjs",
      "require": "./error.js"
    },
    "./error.js": {
      "default": "./error.js"
    },
    "./error.mjs": {
      "default": "./error.mjs"
    },
    "./helpers/*.mjs": {
      "default": [
        "./helpers/*.mjs",
        "./helpers/*/index.mjs"
      ]
    },
    "./helpers/*.js": {
      "default": [
        "./helpers/*.js",
        "./helpers/*/index.js"
      ]
    },
    "./helpers/*": {
      "import": [
        "./helpers/*.mjs",
        "./helpers/*/index.mjs"
      ],
      "require": [
        "./helpers/*.js",
        "./helpers/*/index.js"
      ]
    },
    "./index": {
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./index.js": {
      "default": "./index.js"
    },
    "./index.mjs": {
      "default": "./index.mjs"
    },
    "./lib/*.mjs": {
      "default": [
        "./lib/*.mjs",
        "./lib/*/index.mjs"
      ]
    },
    "./lib/*.js": {
      "default": [
        "./lib/*.js",
        "./lib/*/index.js"
      ]
    },
    "./lib/*": {
      "import": [
        "./lib/*.mjs",
        "./lib/*/index.mjs"
      ],
      "require": [
        "./lib/*.js",
        "./lib/*/index.js"
      ]
    },
    "./pagination": {
      "import": "./pagination.mjs",
      "require": "./pagination.js"
    },
    "./pagination.js": {
      "default": "./pagination.js"
    },
    "./pagination.mjs": {
      "default": "./pagination.mjs"
    },
    "./resource": {
      "import": "./resource.mjs",
      "require": "./resource.js"
    },
    "./resource.js": {
      "default": "./resource.js"
    },
    "./resource.mjs": {
      "default": "./resource.mjs"
    },
    "./resources/*.mjs": {
      "default": [
        "./resources/*.mjs",
        "./resources/*/index.mjs"
      ]
    },
    "./resources/*.js": {
      "default": [
        "./resources/*.js",
        "./resources/*/index.js"
      ]
    },
    "./resources/*": {
      "import": [
        "./resources/*.mjs",
        "./resources/*/index.mjs"
      ],
      "require": [
        "./resources/*.js",
        "./resources/*/index.js"
      ]
    },
    "./streaming": {
      "import": "./streaming.mjs",
      "require": "./streaming.js"
    },
    "./streaming.js": {
      "default": "./streaming.js"
    },
    "./streaming.mjs": {
      "default": "./streaming.mjs"
    },
    "./uploads": {
      "import": "./uploads.mjs",
      "require": "./uploads.js"
    },
    "./uploads.js": {
      "default": "./uploads.js"
    },
    "./uploads.mjs": {
      "default": "./uploads.mjs"
    },
    "./version": {
      "import": "./version.mjs",
      "require": "./version.js"
    },
    "./version.js": {
      "default": "./version.js"
    },
    "./version.mjs": {
      "default": "./version.mjs"
    }
  },
  "bin": "./bin/cli",
  "peerDependencies": {
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  }
}